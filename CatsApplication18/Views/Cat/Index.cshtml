@using CatsApplication18.Entities;
@model IEnumerable<CatsApplication18.Entities.Cat>

    <div class="container">
        <div class="row">
            <div class="col col-md-6">CATS</div>
        <div class="col col-md-6"><a asp-controller="Cat" asp-action="Create" class="btn btn-primary" asp->Create new Cat</a></></div>
        </div>
</div>
<table class="table" type="Primary">
    <thead>
        <tr class="table-primary">
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Breed</th>
            <th scope="col">Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
        <tr>
            <th scope="row"></th>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.Breed</td>
        </tr>
        }
    </tbody>
</table>